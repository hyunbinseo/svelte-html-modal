{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		// FIXME module resolution causing a TypeScript error.
		// Reference https://github.com/sveltejs/kit/issues/11086
		"moduleResolution": "NodeNext"
	}
}
