{
	"name": "svelte-html-modal",
	"description": "Svelte modal component that utilizes the HTML dialog element",
	"version": "1.0.3",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"peerDependencies": {
		"svelte": "^3.59.2 || ^4.0.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.1.1",
		"@sveltejs/kit": "^1.27.6",
		"@sveltejs/package": "^2.2.2",
		"@typescript-eslint/eslint-plugin": "^6.11.0",
		"@typescript-eslint/parser": "^6.11.0",
		"eslint": "^8.53.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte": "^2.35.0",
		"esm-env": "^1.0.0",
		"prettier": "^3.1.0",
		"prettier-plugin-svelte": "^3.1.0",
		"publint": "^0.2.5",
		"svelte": "^4.2.3",
		"svelte-check": "^3.6.0",
		"tslib": "^2.6.2",
		"typescript": "^5.2.2",
		"vite": "^4.5.0"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/hyunbinseo/svelte-html-modal.git"
	},
	"author": "Hyunbin Seo",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/hyunbinseo/svelte-html-modal/issues"
	},
	"homepage": "https://github.com/hyunbinseo/svelte-html-modal#readme"
}
