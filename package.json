{
	"name": "svelte-html-modal",
	"description": "Svelte modal component that utilizes the HTML dialog element",
	"version": "3.0.0-next.8",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && node --run package",
		"preview": "vite preview",
		"postversion": "git push && git push --tags",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "node --run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test": "vitest"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"peerDependencies": {
		"@sveltejs/kit": "^2.5.24",
		"svelte": "^5.0.2"
	},
	"peerDependenciesMeta": {
		"@sveltejs/kit": {
			"optional": true
		}
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.3.1",
		"@sveltejs/kit": "^2.8.2",
		"@sveltejs/package": "^2.3.7",
		"@sveltejs/vite-plugin-svelte": "^4.0.1",
		"@testing-library/svelte": "^5.2.6",
		"eslint": "^9.15.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.46.0",
		"globals": "^15.12.0",
		"happy-dom": "^15.11.6",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.3.2",
		"publint": "^0.2.12",
		"svelte": "^5.2.7",
		"svelte-check": "^4.1.0",
		"typescript": "^5.7.2",
		"typescript-eslint": "^8.15.0",
		"vite": "^5.4.11",
		"vitest": "^2.1.5"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/hyunbinseo/svelte-html-modal.git"
	},
	"author": "Hyunbin Seo",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/hyunbinseo/svelte-html-modal/issues"
	},
	"homepage": "https://github.com/hyunbinseo/svelte-html-modal#readme",
	"packageManager": "pnpm@9.14.2+sha512.6e2baf77d06b9362294152c851c4f278ede37ab1eba3a55fda317a4a17b209f4dbb973fb250a77abc463a341fcb1f17f17cfa24091c4eb319cda0d9b84278387"
}
