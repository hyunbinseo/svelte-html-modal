{
	"name": "svelte-html-modal",
	"description": "Svelte modal component that utilizes the HTML dialog element",
	"version": "3.1.0-next.0",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && node --run prepack",
		"preview": "vite preview",
		"postversion": "git push && git push --tags",
		"prepare": "svelte-kit sync || echo ''",
		"prepack": "svelte-kit sync && svelte-package && publint",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "node --run test:unit -- --run"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"peerDependencies": {
		"svelte": "^5.0.2"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.7",
		"@eslint/js": "^9.22.0",
		"@sveltejs/adapter-auto": "^4.0.0",
		"@sveltejs/kit": "^2.20.0",
		"@sveltejs/package": "^2.3.10",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@testing-library/svelte": "^5.2.7",
		"@types/node": "^22.13.10",
		"eslint": "^9.22.0",
		"eslint-config-prettier": "^10.1.1",
		"eslint-plugin-svelte": "^3.3.1",
		"globals": "^16.0.0",
		"happy-dom": "^17.4.4",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.3.3",
		"publint": "^0.3.9",
		"svelte": "^5.23.1",
		"svelte-check": "^4.1.5",
		"typescript": "^5.8.2",
		"typescript-eslint": "^8.26.1",
		"vite": "^6.2.2",
		"vitest": "^3.0.9"
	},
	"author": "Hyunbin Seo",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/hyunbinseo/svelte-html-modal.git"
	},
	"bugs": {
		"url": "https://github.com/hyunbinseo/svelte-html-modal/issues"
	},
	"homepage": "https://github.com/hyunbinseo/svelte-html-modal#readme",
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		]
	},
	"packageManager": "pnpm@10.2.1+sha512.398035c7bd696d0ba0b10a688ed558285329d27ea994804a52bad9167d8e3a72bcb993f9699585d3ca25779ac64949ef422757a6c31102c12ab932e5cbe5cc92"
}
