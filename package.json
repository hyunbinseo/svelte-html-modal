{
	"name": "svelte-html-modal",
	"description": "Svelte modal component that utilizes the HTML dialog element",
	"version": "2.0.0-next.2",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"peerDependencies": {
		"svelte": "^3.59.2 || ^4.0.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.2.0",
		"@sveltejs/kit": "^2.5.6",
		"@sveltejs/package": "^2.3.1",
		"@sveltejs/vite-plugin-svelte": "^3.1.0",
		"@testing-library/svelte": "^4.1.0",
		"@typescript-eslint/eslint-plugin": "^7.7.0",
		"@typescript-eslint/parser": "^7.7.0",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.37.0",
		"esm-env": "^1.0.0",
		"happy-dom": "^14.7.1",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.3",
		"publint": "^0.2.7",
		"svelte": "^4.2.14",
		"svelte-check": "^3.6.9",
		"tslib": "^2.6.2",
		"typescript": "^5.4.5",
		"vite": "^5.2.9",
		"vitest": "^1.5.0"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/hyunbinseo/svelte-html-modal.git"
	},
	"author": "Hyunbin Seo",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/hyunbinseo/svelte-html-modal/issues"
	},
	"homepage": "https://github.com/hyunbinseo/svelte-html-modal#readme"
}
